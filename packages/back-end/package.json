{
  "name": "back-end",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start:prod": "cross-env NODE_ENV=production nodemon",
    "start:dev": "cross-env NODE_ENV=development nodemon"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "sequelize": "^6.32.1",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.15"
  },
  "nodemonConfig": {
    "ignore": [
      "**/node_modules/**"
    ],
    "delay": 1000,
    "watch": [
      "src/**/*.ts"
    ],
    "ext": "ts,js,mjs,json",
    "verbose": false,
    "exec": "ts-node src/app.ts"
  }
}
